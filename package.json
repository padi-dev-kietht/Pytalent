{
  "name": "nest-mvc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n ./src/databases/config/orm.config.seed.ts",
    "db:drop": "rm src/databases/storage/*.sqlite3",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/databases/config/orm.config.seed.ts",
    "db:refresh": "npm run db:drop; npm run typeorm migration:run; npm run db:seed;",
    "db:refresh:mariadb": "export NODE_ENV=mariadb; npm run db:drop; npm run typeorm migration:run; npm run db:seed;",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production; node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=test; npm run db:refresh; jest --ci --passWithNoTests;",
    "test:e2e": "export NODE_ENV=test; npm run db:refresh; jest test/e2e --ci --passWithNoTests",
    "test:unit": "export NODE_ENV=test; npm run db:refresh; jest test/unit --ci --passWithNoTests",
    "test:validation": "export NODE_ENV=test; npm run db:refresh; jest test/validation --ci --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "typeorm-ts-node-esm -d ./src/databases/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "i18n": "^0.15.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.4",
    "typedi": "^0.10.0",
    "typeorm": "0.3.11",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nestjs/throttler": "^4.0.0",
    "@types/connect-flash": "^0.0.37",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^6.0.0",
    "@types/express-session": "^1.17.5",
    "@types/i18n": "^0.13.6",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "4.5.2"
  }
}
